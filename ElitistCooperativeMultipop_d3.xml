<?xml version="1.0"?>
<!DOCTYPE simulator [
<!ATTLIST algorithm id ID #IMPLIED>
<!ATTLIST problem id ID #IMPLIED>
<!ATTLIST measurements id ID #IMPLIED>
]>
<simulator>
    <algorithms>
        <algorithm id="clusteringPSO1_f1" class="clustering.DataClusteringPSO">
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="1"/>
            <iterationStrategy class="clustering.pso.iterationstrategies.StandardDataClusteringIterationStrategy">
                <boundaryConstraint class="problem.boundaryconstraint.CentroidBoundaryConstraint">
                    <delegate class="problem.boundaryconstraint.ClampingBoundaryConstraint"/>
                </boundaryConstraint>
            </iterationStrategy>
            <initialisationStrategy class="algorithm.initialisation.DataDependantPopulationInitializationStrategy">
                <delegate class="algorithm.initialisation.ClonedPopulationInitialisationStrategy"/>
                <entityNumber value="50"/>
                <entityType class="clustering.entity.ClusterParticle">
                    <positionInitialisationStrategy class="entity.initialization.RandomBoundedInitializationStrategy"/>
                    <centroidInitialisationStrategy class="entity.initialization.StandardCentroidInitializationStrategy"/>
                </entityType>
            </initialisationStrategy>
        </algorithm>
        
         <algorithm id="cooperativePSO1_f1" class="clustering.CooperativePSO">
            <iterationStrategy class="pso.multiswarm.CooperativeMultiswarmIterationStrategy" isElitist="false">
                <delegate class="pso.multiswarm.StandardClusteringMultiSwarmIterationStrategy"/>
            </iterationStrategy>
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="1000"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
            <addPopulationBasedAlgorithm idref="clusteringPSO1_f1"/>
        </algorithm>
        
    </algorithms>

    <problems>
        <problem id="quantization_d3_s1_f1" class="problem.QuantizationErrorMinimizationProblem" domain="R(-10:10)" numberOfClusters="8" sourceURL="TemporalClustering/d3_s1_f1.arff">
            <window class="clustering.SlidingWindow" windowSize="80" slideFrequency="10"/>
        </problem>
    </problems>
    <measurements id="fitness" class="simulator.MeasurementSuite" resolution="10">
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="0"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="1"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="2"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="3"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="4"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="5"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="6"/>
        <addMeasurement class="measurement.multiple.ClusterCentroids" dimension="7"/>
        <addMeasurement class="measurement.clustervalidity.RayTuriValidityIndex"/>
        <addMeasurement class="measurement.clustervalidity.InterClusterDistance"/>
        <addMeasurement class="measurement.clustervalidity.IntraClusterDistance"/>
        <addMeasurement class="measurement.single.Fitness"/>
    </measurements>
    <simulations>
        <simulation samples="1">
            <algorithm idref="cooperativePSO1_f1"/>
            <problem idref="quantization_d3_s1_f1"/>
            <measurements idref="fitness" />
	    <output format="TXT" file="data/2nonElitistCooperativeMultipop_d3_s1_f1.txt" />
