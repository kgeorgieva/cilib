<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.scala.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/scala-project/1">
            <name>simulator</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <source-roots><root name="Java Source" id="src.java.dir"></root><root name="Configuration" id="sources.sbt.project"></root></source-roots>
            <test-roots>
                <root name="Java Test Sources" id="file.reference.test-java"></root>
                <root name="Test Resources" id="test.resources.dir"></root>
                <root name="Java Source" id="src.java.dir"></root>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1"><reference>
            <foreign-project>library</foreign-project>
            <artifact-type>jar</artifact-type>
            <script>build.xml</script>
            <target>jar</target>
            <clean-target>clean</clean-target>
            <id>jar</id>
          </reference></references>
    </configuration>
</project>